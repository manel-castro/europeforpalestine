{
  "name": "corporative-web",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Manel Castro",
  "license": "MIT",
  "scripts": {
    "start": "node ./server/server.js",
    "cp": "cp -v ./.prettierignore ./server && echo ok",
    "ts": "yarn run types-check",
    "start:dev": "webpack-cli serve -c webpack.config.dev.js --hot",
    "types-check": "tsc -w",
    "build": "webpack --config webpack.config.prod.js --progress && cp -v ./_redirects ./build && echo ok"
  },
  "dependencies": {
    "eslint-webpack-plugin": "^2.5.3",
    "express": "^4.17.1",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^16.3.5",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "5.1.6",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "core-js": "3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "pure": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  }
}
